@using ProyectoWeb.ClientesWS
@using ProyectoWeb.EmpleadosWS
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css" integrity="sha384-GJzZqFGwb1QTTN6wy59ffF1BuGJpLSa9DkKMp0DgiMDm4iYMj70gZWKYbI706tWS" crossorigin="anonymous">
    <title>Listar Clientes </title>
</head>
<body>
    <div>
        <h5 class="display-5">Clientes @((EMPLEADOS)Session["usuario"]).USUARIO</h5>

        <table class="table">

            @*@foreach (WebOWIN2.entityFramework.AspNetRoles item in ViewBag.lista)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                </tr>
            }*@

            <tr>
                <td>&nbsp;</td>
            </tr>
            <tr>
                <td>Rut</td>
                <td>Nombre</td>
                <td>Teléfono</td>
            </tr>
            @foreach(CLIENTES cliente in ViewBag.listaclientes) {
                <tr>
                    <td>@cliente.RUT</td>
                    <td>@cliente.NOMBRE</td>
                    <td>@cliente.TELEFONO</td>
                </tr>
            }
        </table>

        <a href="/Cliente/IngresarCliente/"  class="btn btn-secondary btn-sm">Ingresar Cliente</a>

    </div>
</body>
</html>
